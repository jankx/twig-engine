<header id="{{ header_id }}" class={{ header_class | join(' ') }}>
    {{ do_action('jankx_component_before_header_content') }}

    {{ jankx_open_container('c-header-container', 'jankx_component_header_container') }}

        {{ do_action('jankx_component_before_header') }}
        {{ content | raw }}
        {{ do_action('jankx_component_after_header') }}

    {{ jankx_close_container('jankx_component_header_container') }}

    {{ do_action('jankx_component_after_header_content') }}
</header>
