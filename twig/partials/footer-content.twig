<footer id="jankx-site-footer" class="jankx-site-footer">
    {{ do_action('jankx_template_before_footer_content') }}

        <div class="jankx-copyright">
            {{ jankx_open_container }}
                <div class="text">
                    {{ copyright | raw }}.
                    {% if enable_credit is same as true %}
                        {{ credit_message | raw }}
                    {% endif %}
                </div>
            {{ jankx_close_container }}
        </div>


    {{ do_action('jankx_template_after_footer_content') }}
</footer>
